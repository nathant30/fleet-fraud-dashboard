{
  "name": "fleet-fraud-dashboard-api",
  "version": "1.0.0",
  "description": "Backend API for Fleet Fraud Detection Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:frontend": "vite",
    "build:frontend": "vite build",
    "preview:frontend": "vite preview",
    "test": "jest",
    "test:frontend": "jest --config jest.frontend.config.js",
    "test:api": "jest tests/api.test.js",
    "test:database": "jest tests/database.test.js",
    "test:integration": "jest tests/integration.test.js",
    "test:fraud": "jest tests/fraud-detection.test.js",
    "test:qa": "node tests/qa-runner.js",
    "test:phase4": "node tests/phase4/Phase4QAOrchestrator.js",
    "test:phase4:frontend": "jest tests/phase4/frontend --config jest.config.js --testTimeout=180000",
    "test:phase4:database": "jest tests/phase4/database --config jest.config.js --testTimeout=300000",
    "test:phase4:theming": "jest tests/phase4/frontend/theming --config jest.config.js --testTimeout=120000",
    "test:phase4:reporting": "jest tests/phase4/frontend/reporting --config jest.config.js --testTimeout=180000",
    "test:phase4:portal": "jest tests/phase4/frontend/developer-portal --config jest.config.js --testTimeout=150000",
    "test:phase4:i18n": "jest tests/phase4/frontend/i18n --config jest.config.js --testTimeout=120000",
    "test:phase4:cluster": "jest tests/phase4/database/multi-region --config jest.config.js --testTimeout=300000",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:setup": "node server/scripts/setupDatabase.js",
    "db:migrate": "node server/scripts/migrate.js",
    "db:seed": "node server/scripts/seed.js",
    "db:reset": "node server/scripts/reset.js"
  },
  "keywords": [
    "fleet",
    "fraud",
    "detection",
    "express",
    "supabase"
  ],
